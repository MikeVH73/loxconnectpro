{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ],
  "git": {
    "deploymentEnabled": true,
    "silent": false
  },
  "github": {
    "enabled": true,
    "silent": false,
    "autoAlias": true,
    "deployOnPush": true,
    "autoJobCancelation": true,
    "productionBranch": "deployment-july-2025"
  },
  "public": false,
  "regions": ["fra1"],
  "cleanUrls": true,
  "trailingSlash": false,
  "ignoreCommand": null,
  "buildCommand": "npm run build",
  "outputDirectory": ".next"
}